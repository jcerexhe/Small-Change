<!DOCTYPE html>
<html>
	<head>
 	 <title><%= full_title(yield(:title)) %></title>
   	<%= favicon_link_tag(source='greenlogo.png', width: 300) %>
    <%= stylesheet_link_tag    'application', media: 'all'%>
    <%= javascript_include_tag 'application'%>
   	<%= csrf_meta_tags %>

    <link href='https://fonts.googleapis.com/css?family=Josefin+Sans' rel='stylesheet' type='text/css'>

    <% if content_for?(:head) %>
      <%= yield(:head) %>
    <% end %>

 	  <%= render 'shared/googleanalytics' %>
	</head>
	<body>
  	<%= render 'shared/ptengine' %>   
		<%= render 'layouts/header' unless current_page?(submission_path(:id)) %>

<!-- 		<p class = "notice"><%#= notice %></p>
		<p class = "alert"><%#= alert %></p> -->

		<%= yield %>
			
		<%= render 'layouts/footer' unless current_page?(submission_path(:id)) %>
	</body>
</html>
